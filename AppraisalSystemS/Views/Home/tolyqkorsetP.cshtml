        @Html.DisplayFor(modelItem => item.begsto)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.begtys)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.pryzhvdlin)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.standinam)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.kistdinamlev)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.kistdinamprav)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.podtyagiv)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.pres)
