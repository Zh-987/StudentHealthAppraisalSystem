        @Html.DisplayFor(modelItem => item.metangran)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.chelnochnbeg)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.testkuper)
    </td>


    <td>
        @Html.DisplayFor(modelItem => item.Comp_Organ.comp_organ_name)
    </td>
    <td>
        @Html.ActionLink("Өзгерту", "EditCompPedagog", new { id = item.Pedag_test_id }) |
        @Html.ActionLink("Нақтырақ", "DetailsCompPedagog", new { id = item.Pedag_test_id }) |
        @Html.ActionLink("Өшіру", "DeleteCompPedagog", new { id = item.Pedag_test_id })
    </td>
</tr>
    }

</table>



<div id="container" style="margin-left:20px">
    <p></p>
    <p></p>
    <div class="pagination" style="margin-left:400px">
        Бет @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber)
        /    @Model.PageCount  @Html.PagedListPager(Model, page => Url.Action("tolyqkorsetP", new { page }))
    </div>
</div>

